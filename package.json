{
  "name": "egle-eye",
  "version": "1.0.0",
  "description": "Simple DHT11 sensor web monitor for Raspberry Pi",
  "main": "egle-eye.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon egle-eye.js",
    "app": "sudo node egle-eye.js",
    "app-win": "node egle-eye.js",
    "check-cpu": "ps aux | sort -k4 -nr | head -10",
    "check-mem": "ps aux | sort -k3 -nr | head -10",
    "check-disk": "df -h",
    "check-temp": "vcgencmd measure_temp",
    "start": "pm2 start egle-eye.js -i 1",
    "stop": "pm2 stop egle-eye.js",
    "restart": "pm2 restart egle-eye.js",
    "delete": "pm2 delete egle-eye.js",
    "min-1": "node commandSystem/min_css_js.js",
    "min-2": "node commandSystem/delete_min_js.js",
    "min-3": "node commandSystem/min_routes.js",
    "min-4": "node commandSystem/min_mymodule.js",
    "min-c": "node commandSystem/min_commandSystem.js",
    "min-all": "npm run min-1 && npm run min-2 && npm run min-3 && npm run min-4 && npm run min-c",
    "create-f": "node commandSystem-min/create_folders.js",
    "create-i": "node commandSystem-min/create_dbCollectionIndex.js",
    "create-all": "npm run create-f && npm run create-i",
    "clear-db-docs": "node commandSystem-min/clear_db_docs.js",
    "clear-db-data": "node commandSystem-min/clear_db_data.js",
    "clear-db-entire": "node commandSystem-min/clear_db_entire.js",
    "kill-win": "taskkill /f /im node.exe",
    "kill-mac": "pkill -9 node",
    "kill-linux": "pkill -9 node",
    "kill-all": "sudo pkill -9 -f 'sudo node egle-eye.js' && sudo pkill -9 -f 'node egle-eye.js' && sudo pkill -9 -f 'sh -c sudo node egle-eye.js'",
    "node-v": "node --version",
    "git1": "git add . && git commit -m \"%date% %time%\" && git push origin main",
    "git-force-pull": "git reset --hard HEAD && git clean -fd && git pull",
    "git-gc": "git gc"
  },
  "dependencies": {
    "archiver": "^7.0.1",
    "axios": "^1.8.4",
    "bcrypt": "^5.1.1",
    "connect-flash": "^0.1.1",
    "crypto-js": "^4.2.0",
    "dotenv": "^17.2.1",
    "ejs": "^3.1.10",
    "esbuild": "^0.25.5",
    "express": "^4.18.2",
    "express-session": "^1.18.1",
    "http-proxy-middleware": "^3.0.5",
    "javascript-obfuscator": "^4.1.1",
    "lowdb": "^7.0.1",
    "luxon": "^3.5.0",
    "multer": "^2.0.0",
    "node-schedule": "^2.1.1",
    "node-telegram-bot-api": "^0.66.0",
    "nodemailer": "^6.10.1",
    "pigpio-client": "^1.5.2",
    "readline": "^1.3.0",
    "request": "^2.88.2",
    "sharp": "^0.34.4",
    "socket.io": "^4.8.1",
    "svg-captcha": "^1.4.0"
  },
  "keywords": [
    "raspberry-pi",
    "dht11",
    "sensor",
    "web-monitor",
    "simple"
  ],
  "author": "Pi Learning Project",
  "license": "MIT"
}
